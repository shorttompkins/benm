{
    "name": "myapp",
    "description": "Boilerplate web app using node, express, mongodb, backbone, marionette.  Tooling includes Grunt, Bower, Browserify, etc.",
    "version": "0.0.1",
    "author": "Jason Krol",
    "repository": {
        "type": "git",
        "url": "https://github.com/jkat98/benm.git"
    },
    "engines": {
        "node": "0.10.x"
    },
    "scripts": {
        "start": "node server.js"
    },
    "dependencies": {
        "express": "latest",
        "mongoose": "~3.8.3",
        "handlebars-runtime": "~1.0.12",
        "express3-handlebars": "~0.5.0",
        "MD5": "~1.2.0"
    },
    "devDependencies": {
        "bower": "~1.2.8",
        "grunt": "~0.4.1",
        "grunt-contrib-concat": "~0.3.0",
        "grunt-contrib-jshint": "~0.7.2",
        "grunt-contrib-uglify": "~0.2.7",
        "grunt-bower-task": "~0.3.4",
        "grunt-nodemon": "~0.1.2",
        "karma-script-launcher": "~0.1.0",
        "karma-chrome-launcher": "~0.1.1",
        "karma-html2js-preprocessor": "~0.1.0",
        "karma-firefox-launcher": "~0.1.2",
        "karma-jasmine": "~0.1.4",
        "karma-requirejs": "~0.2.0",
        "karma-coffee-preprocessor": "~0.1.1",
        "karma-phantomjs-launcher": "~0.1.1",
        "karma": "~0.10.8",
        "grunt-contrib-copy": "~0.4.1",
        "grunt-contrib-clean": "~0.5.0",
        "grunt-browserify": "~1.3.0",
        "load-grunt-tasks": "~0.2.0",
        "time-grunt": "~0.2.3",
        "grunt-contrib-watch": "~0.5.3",
        "grunt-concurrent": "~0.4.2",
        "grunt-karma": "~0.6.2",
        "grunt-contrib-less": "~0.8.3",
        "grunt-contrib-handlebars": "~0.6.0",
        "grunt-contrib-cssmin": "~0.7.0",
        "hbsfy": "~1.0.0",
        "grunt-shell-spawn": "~0.3.0",
        "gulp": "~3.3.0",
        "gulp-util": "~2.2.9",
        "gulp-load-tasks": "~0.1.1",
        "gulp-less": "~1.1.7",
        "gulp-concat": "~2.1.7",
        "gulp-clean": "~0.2.2",
        "gulp-browserify": "~0.2.4",
        "gulp-minify-css": "~0.1.0",
        "gulp-uglify": "~0.1.0",
        "gulp-watch": "~0.3.3",
        "gulp-nodemon": "0.0.3",
        "gulp-exec": "~1.0.3",
        "gulp-karma": "0.0.1",
        "gulp-jshint": "~1.3.4",
        "gulp-csslint": "~0.1.3",
        "gulp-jscs": "~0.1.3",
        "browserify-shim": "~3.1.4"
    },
    "browser": {
        "jquery": "bower_components/jquery/jquery.js",
        "underscore": "bower_components/underscore/underscore.js",
        "backbone": "bower_components/backbone/backbone.js",
        "backbone.marionette": "bower_components/backbone.marionette/backbone.marionette.js"
    },
    "browserify-shim": {
        "jquery": "$",
        "underscore": "_",
        "backbone": {
            "exports": "Backbone",
            "depends": ["bower_components/underscore/underscore.js:underscore"]
        },
        "backbone.marionette": {
            "exports": "Marionette",
            "depends": ["jquery:$", "underscore:_", "backbone:Backbone"]
        }
    }
}
